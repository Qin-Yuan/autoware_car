<?xml version="1.0"?>
<root>
    <include ros_pkg="autoware_behavior_tree" path="behavior_trees/autoware_bt0_navigation.xml"/>
    <!-- autoware nav 功能分类 三级 -->
    <BehaviorTree ID="ServiceSwitch">
        <Switch2 name="ServiceMission" variable="{child_type}" case_1="to_goal" case_2="with_people">
            <SubTree ID="NavToGoal" __shared_blackboard="true"/>
            <AlwaysSuccess/>
            <!-- 其他未设置任务默认返回失败 -->
            <AlwaysFailure/>
        </Switch2>
    </BehaviorTree>
</root>
